  "symbolRules": {
    "general.avatar": {
      "exclude": [
        { "type": "regex", "value": "^custom_" }
      ]
    },
    "general.outfit": {
      "exclude": [
        { "type": "regex", "value": "^custom_" }
      ]
    },
    "general.name_icon": {
